section.catalog
  .container
    ul.filter.list-reset
      li.filter__item
        button.btn-reset.filter__btn(aria-label="Фильтр")
          span.filter__subtitle Фильтр
          svg(aria-hidden="true" width="15px" height="7.5px")
            use(xlink:href="img/sprite.svg#arrowBottom")

        ul.filter__category.list-reset
